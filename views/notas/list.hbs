<div class="container">
    <h1>LIST</h1>
    {{#each notas}}
        <h1>{{title}}</h1>
        <h1>{{description}}</h1>
        <div class="c_botones">
            <button type="submit"><a href="/notas/edit/{{id}}">EDIT</a></button>
            <button type="submit"><a href="/notas/delete/{{id}}">Delete</a></button>
        </div>
    {{else}}
        <h1>Esta vacio</h1>
    {{/each}}
</div>